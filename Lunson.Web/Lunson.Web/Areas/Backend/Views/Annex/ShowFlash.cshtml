@model Lunson.Domain.Entities.Annex
@{
    ViewBag.Title = "Flash显示";
}

<div id="flashdiv" style="text-align:center;">
    <object id="flashid" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,29,0" style="width:606px;height:362px;">
        <param name="movie" value="@Model.Url">
        <param name="quality" value="high">
        <param name="wmode" value="transparent">
        <embed src="@Model.Url"  style="width:606px;height:362px;" wmode="transparent" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash">
    </object>
</div>
